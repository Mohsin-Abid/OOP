#include <iostream> 
using namespace std;
int main() { 
 int a,b; 
  cout << "Enter two integers: ";
  cin>>a>>b;
   if ( a == b )
        cout << "These numbers are equal." << endl; 
    if ( a > b )
        cout << a << " is larger." << endl;  
    if ( b > a )
   		 cout << b << " is larger." << endl; 

   return 0;
}
