#include <iostream> 
using namespace std;
int main() { 
	int n=0;
	cout << "integer\tsquare\tcube\n"; 
	
	 cout << n << '\t' << n * n << '\t' << n * n * n << "\n";
	  
	  n=1;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n"; 
	  
	  n=2;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=3;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=4;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=5;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=6;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=7;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=8;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=9;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
	  
	  n=10;
	  cout << n << '\t' << n * n << '\t'<< n * n * n << "\n";
   return 0;
}
